<% include ./partials/header %>
<h1>Index Page</h1>  

<% blogs.forEach((blog) => {%>
    <div>
        <h2><%=blog.title%></h2>
        <img src="<%= blog.image%>" ><br>
        <span><%= blog.created %></span>
        <p><%= blog.body %></p>
        <a href="/BLOGS/<%= blog._id %>">Read</a>
    </div>
<% })%>

<% include ./partials/footer %>

